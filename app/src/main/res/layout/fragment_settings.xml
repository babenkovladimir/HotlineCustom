<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.ProfileFragmentJ"
    >
  <Button
      android:id="@+id/btSignOutSettingsFragment"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="412dp"
      android:layout_marginEnd="16dp"
      android:layout_marginTop="8dp"
      android:text="SignOut"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      />

  <Button
      android:id="@+id/btBattaryState"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"

      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"

      android:layout_marginTop="8dp"
      android:text="Check internetConnection"
      app:layout_constraintEnd_toStartOf="@+id/btSignOutSettingsFragment"
      app:layout_constraintHorizontal_bias="0.571"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      />

  <TextView
      android:id="@+id/tvBuildVersionSettingsFragment"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="16dp"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      android:gravity="center_vertical"
      android:textSize="24sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.502"
      app:layout_constraintStart_toStartOf="parent"
      tools:text="Build version"
      />


</android.support.constraint.ConstraintLayout>