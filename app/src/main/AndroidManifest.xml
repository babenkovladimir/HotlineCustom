<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vladimirbabenko.hotlinecustom">

  <uses-permission android:name="android.permission.INTERNET"/>

  <application
      android:name=".base.App"
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity
        android:name=".LoginActivity"
        android:label="Login">
    </activity>
    <activity
        android:name=".j.LoginActivityJ"
        android:label="LoginJ">
    </activity>
    <!-- <activity android:name=".base.BaseActivity"> -->
    <!-- </activity> -->
    <activity
        android:name=".j.MainScreenActivityJ"
        android:label="MainScreenJ">
    </activity>
    <activity
        android:name=".MainScreenActivity"
        android:label="MainScreen">
    </activity>
    <activity
        android:name=".TermsActivity"
        android:label="Terms">
    </activity>

    <receiver
        android:name=".utils.MyReceiverCheckInternetConnectionJ"
        android:enabled="true"
        android:exported="true">
      <intent-filter>
        <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
      </intent-filter>
    </receiver>
  </application>

</manifest>